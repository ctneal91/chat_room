<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= form_for @user do |f| %>
  <div class="sign_up_form">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

  <div class="sign_up_form">
    <%= f.label :password %>
    <%= f.text_field :password %>
  </div>

  <div class="sign_up_form">
    <%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation %>
  </div>

  <div class="btn">
    <%= f.submit "Sign Up" %>
  </div>

<% end %>
