<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_tag do %>
  <div class="sign_in_form">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>

  <div class="sign_in_form">
    <%= label_tag :password %>
    <%= text_field_tag :password %>
  </div>

  <div class="btn btn-primary">
    <%= submit_tag "Sign In" %>
  </div>

<% end %>
