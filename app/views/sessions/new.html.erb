<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_tag do %>
  <div class="sign_in_form">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>

  <div class="sign_in_form">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>

  <div class="btn">
    <%= submit_tag "Sign In" %>
  </div>

  <div class="btn">
    <%= link_to 'Sign Up', sign_up_path %>
  </div>

  <%= @users.each do |user| %>
    <%= user.email %>
  <% end %>


<% end %>
